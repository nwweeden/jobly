{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobNew.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "salary": {
        "type": "integer",
        "minimum": 0
      },
      "equity": {
        "type": "number",
        "maximum": 1
      },
      "company_handle": {
        "type": "string",
        "maxLength": 10
      }
    },
    "additionalProperties": false,
    "required": [
      "title",
      "company_handle"
    ]
  }